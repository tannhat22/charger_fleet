<?xml version='1.0' ?>

<launch>

  <node pkg="charger_fleet_server_ros2"
      exec="charger_fleet_server_ros2"
      name="fake_server_node"
      output="both">

    <param name="fleet_name" value="amr_vdm"/>
    <!-- ros topic -->
    <param name="fleet_state_topic" value="fleet_charger_state"/>
    <param name="mode_request_topic" value="mode_charger_request"/>
    <!-- dds topic -->
    <param name="dds_domain" value="52"/>
    <param name="dds_charger_state_topic" value="charger_state"/>
    <param name="dds_mode_request_topic" value="mode_charger_request"/>
    <param name="update_state_frequency" value="5.0"/>
    <param name="publish_state_frequency" value="1.0"/>

  </node>

</launch>

